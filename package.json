{
    "name": "obsidian-latex-suite",
    "version": "1.9.8",
    "description": "Make typesetting LaTeX math as fast as handwriting through snippets, text expansion, and editor enhancements",
    "main": "dist/extension.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "dev:full": "concurrently \"npm run dev:web\" \"npm run dev\"",
        "dev:web": "dotenv -e .env -- web-ext run"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@codemirror/autocomplete": "^6.0.0",
        "@codemirror/commands": "^6.0.0",
        "@codemirror/lang-javascript": "^6.0.0",
        "@codemirror/language": "https://github.com/lishid/cm-language",
        "@codemirror/lint": "^6.0.0",
        "@codemirror/search": "^6.0.0",
        "@codemirror/state": "^6.0.0",
        "@codemirror/view": "^6.0.0",
        "@lezer/common": "^1.0.3",
        "@popperjs/core": "2.11.5",
        "@replit/codemirror-vim": "^6.3.0",
        "@types/node": "^24.1.0",
        "@typescript-eslint/eslint-plugin": "^6.18.0",
        "@typescript-eslint/parser": "^6.18.0",
        "builtin-modules": "^3.2.0",
        "codemirror": "^6.0.0",
        "concurrently": "^9.2.0",
        "dotenv": "^17.0.1",
        "esbuild": "^0.20.2",
        "esbuild-plugin-inline-import": "^1.0.2",
        "eslint": "^8.56.0",
        "js-base64": "^3.7.5",
        "obsidian": "^0.15.0",
        "tslib": "2.3.1",
        "typescript": "^5.8.3",
        "web-ext": "^8.8.0"
    },
    "dependencies": {
        "set.prototype.difference": "^1.1.5",
        "set.prototype.intersection": "^1.1.5",
        "valibot": "^0.24.1",
        "winston": "^3.17.0"
    }
}
